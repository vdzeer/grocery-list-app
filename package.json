{
  "name": "grocerylistapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tsc --noEmit",
    "lint:style": "eslint \"**/*.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,jsx,tsx}\" --quiet --fix",
    "start": "react-native start",
    "pod": "cd ios && pod install && cd ..",
    "test": "jest",
    "prepare": "husky",
    "prettify": "prettier --write \"**/*.{js,ts,jsx,tsx}\"",
    "server": "json-server --watch db.json --host 0.0.0.0 --port 3000"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/themed": "^1.1.73",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.1.1",
    "@tanstack/react-query": "^5.65.1",
    "axios": "^1.7.9",
    "formik": "^2.4.6",
    "lucide-react-native": "^0.518.0",
    "react": "18.3.1",
    "react-dom": "^19.1.0",
    "react-native": "0.77.0",
    "react-native-bootsplash": "^6.3.3",
    "react-native-buffer": "^6.0.3",
    "react-native-config": "^1.5.3",
    "react-native-gesture-handler": "^2.22.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^9.2.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.2.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.5.0",
    "react-native-svg": "^15.12.0",
    "react-native-svg-transformer": "^1.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/runtime": "^7.26.7",
    "@eslint/compat": "^1.2.6",
    "@react-native-community/cli": "15.1.3",
    "@react-native-community/cli-platform-android": "15.1.3",
    "@react-native-community/cli-platform-ios": "15.1.3",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-native-get-random-values": "^1.8.2",
    "@types/react-test-renderer": "^19.0.0",
    "@types/styled-components-react-native": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^15.4.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "19.0.0",
    "typescript": "5.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
